{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://plumviajes.sanity.studio"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Authorization, Content-Type"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "connect-src 'self' https://api.sanity.io https://cdn.sanity.io https://aws-qa1.ola.com.ar; img-src 'self' https://cdn.sanity.io data: blob:;"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/clean-old-tagged-packages",
      "schedule": "0 0 * * *"
    }
  ]
}
